@model System.Data.DataTable
@using System.Data;
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Scripts{
    <script>
        $(function () {
            $("#example1").DataTable();
        });
    </script>
    @*<script src="~/Assets/js/BangLuongDonVi.js"></script>*@
    <link href="~/Assets/css/Modal.css" rel="stylesheet" />
    <script>
        function myFunction() {
            $("#divLoading").show();
            window.location.href = "/ImportPHLD/ImportexcelToDb";
        }
    </script>
    <style>
        #example1_filter {
            float: right;
        }
    </style>

}

<section class="content-header">
    <h1 style="width:20%; float:left">
        Nhập file
    </h1>
    <!--<![endif]-->
    @using (Html.BeginForm("ImportexcelToDb", "ImportPHLD", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <input style="float:left; width:auto" class="btn btn-default" type="file" id="file" name="file">
        <button type="submit" id="Submit" name="Submit" value="Submit" class="btn btn-primary" onclick="myFunction()">Đọc file</button>
    }
</section>
<section class="content-header">
    <div class="box">
        <!-- /.box-header -->
        <div class="box-body">
            <table id="example1" class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th>Dòng</th>
                        <th>Năm</th>
                        <th>Tháng</th>
                        <th>Nhân Sự ID</th>
                        @*<th>Số lượng 1- Splitter lắp đặt, hàn nối thông tuyến toàn trình (đơn giá 150.000 đ)</th>
                        <th>Số lượng 2- Splitter hàn nối thông tuyến toàn trình (đơn giá 120.000 đ)</th>*@
                        <th>Lương</th>
                    </tr>
                </thead>
                <tbody>
            </table>
        </div>
        <!-- /.box-body -->
    </div>
</section>



